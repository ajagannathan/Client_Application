        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="20"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>


        <!-- Text box for termial-->
        <!--<ScrollViewer Grid.Column="1" Grid.Row="2" Grid.RowSpan="7" Grid.ColumnSpan="8" HorizontalScrollBarVisibility="Visible">
        </ScrollViewer>-->
        <!--Controls for Server Connection-->
        <TextBox x:Name="TerminalTextBox" Grid.Column="1" Grid.Row="2" Grid.RowSpan="7" Grid.ColumnSpan="8" IsReadOnly="True" Background="Black" 
                     Foreground="#FF02FF02" HorizontalAlignment="Left" Width="380" TextWrapping="Wrap" ScrollViewer.CanContentScroll="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" TextChanged="TerminalTextBox_TextChanged"/>

        <TextBlock Grid.Column="1" HorizontalAlignment="Right" Grid.Row="1" Text="HostIP" Margin="5"/>
        <TextBox x:Name="HostIP_TextBox" Text="127.0.0.1" Grid.Column="2" Grid.Row="1" Margin="5" Grid.ColumnSpan="2"
                 Width="80"/>
        <TextBlock Grid.Column="4"  HorizontalAlignment="Right" Grid.Row="1" Text="Port" Margin="10,5,5,5"/>
        <TextBox x:Name="Port_TextBox" Text="8888" Grid.Column="5" Grid.Row="1" Margin="5" Grid.ColumnSpan="2"
                 Width="50"/>
        <Button x:Name="Connect_Button" Grid.Column="7" Grid.Row="1"
                Grid.ColumnSpan="1" HorizontalAlignment="Left" Width="80" Content="Connect" Margin="5" Click="Connect_Button_Click"/>
        <Button x:Name="Disconnect_Button" Grid.Column="7" Grid.Row="1"
                Grid.ColumnSpan="1" Width="80" Content="Disconnect" Margin="5" Visibility="Hidden" Click="Disconnect_Button_Click"/>


        <Button x:Name="Initialize_Button" IsEnabled="False" Grid.Column="9" Grid.Row="1"
