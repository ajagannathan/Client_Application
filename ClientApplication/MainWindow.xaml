                <CheckBox x:Name="WaferOnEndEffector_1" Grid.Column="6" Grid.Row="5" HorizontalAlignment="Center" Content="Wafer on Arm 1" Foreground="White" Margin="5" Width="120"/>
                <CheckBox x:Name="EdgeGripper1_switchON" Grid.Column="6" Grid.Row="6" HorizontalAlignment="Center" Content="Edge Gripper 1 ON" Foreground="White" Margin="5" Width="120"/>

